<h1>Code Start</h1>

<% if user_signed_in? %>
    <div class="logout-btn">
        <%= link_to "ログアウト", destroy_user_session_path, method: :delete, data: { turbo_method: :delete, turbo_confirm: "本当にログアウトしますか？" } %>
    </div>
<% else %>
    <div class="authentication_btn">
        <%= link_to "新規登録", new_user_registration_path %>
        <%= link_to "ログイン", new_user_session_path %>
    </div>
<% end %>

